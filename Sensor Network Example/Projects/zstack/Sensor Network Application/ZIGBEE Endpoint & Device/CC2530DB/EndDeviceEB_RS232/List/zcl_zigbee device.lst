    277          /*********************************************************************
    278           * @fn      zclZigbeeDevice_HandleKeys
    279           * @brief   Handles all key events for this device.
    280           * @param   shift - true if in shift/alt.
    281           * @param   keys - bit field for key events. 
    282           *          Valid entries: HAL_KEY_SW_4(MCU-K2)
    283           *                         HAL_KEY_SW_1(MCU-K1)
    284           */
    285          static void zclZigbeeDevice_HandleKeys( byte shift, byte keys )
    286          {
    287            zAddrType_t dstAddr;
    288            (void)shift;  // Intentionally unreferenced parameter
    289            
    290            
    291            if ( keys & HAL_KEY_SW1 ) // Using timer to control send sensor data event
    292            {    
    293          #if defined M140 || defined M200
    294                osal_start_timerEx( zclZigbeeDevice_TaskID, SENSOR_SEND_MSG_EVT, SEND_DISPLAY_TIMER); // Start the OSAL timer
    295                osal_set_event( zclZigbeeDevice_TaskID, SENSOR_SEND_MSG_EVT ); // Action the SENSOR_SEND_MSG_EVT
    296          #endif
    297            }
    298            
    299            if ( keys & HAL_KEY_SW2 ) // Atuo binding the device 
    300            {
    301              HalLedSet ( HAL_LED_3, HAL_LED_MODE_BLINK );
    302          
    303              // Initiate a Match Description Request (Service Discovery)
    304              dstAddr.addrMode = AddrBroadcast;
    305              dstAddr.addr.shortAddr = NWK_BROADCAST_SHORTADDR;
    306              ZDP_MatchDescReq( &dstAddr, 
    307                                NWK_BROADCAST_SHORTADDR,
    308                                ZCL_HA_PROFILE_ID,
    309                                ZCLZIGBEEDEV_BINDINGLIST, 
    310                                bindingOutClusters,
    311                                0, 
    312                                NULL, // No incoming clusters to bind
    313                                FALSE );
    314            }
    315          }
