    256          void zclZigbeeDevice_ProcessZDOMsgs( zdoIncomingMsg_t *inMsg )
    257          {
    258            switch ( inMsg->clusterID )
    259            {
    260              case End_Device_Bind_rsp:
    261                if ( ZDO_ParseBindRsp( inMsg ) == ZSuccess )
    262                {
    263                  HalLedSet( HAL_LED_1, HAL_LED_MODE_ON ); // Light LED
    264                }
    265                #if defined(BLINK_LEDS)
    266                else
    267                {      
    268                  HalLedSet ( HAL_LED_1, HAL_LED_MODE_FLASH ); // Flash LED to show failure
    269                }
    270                #endif
    271                break;
    272          
    273              case Match_Desc_rsp:
    274                {
    275                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    276                  if ( pRsp )
    277                  {
    278                    if ( pRsp->status == ZSuccess && pRsp->cnt )
    279                    {
    280                      zclZigbeeDevice_DstAddr.addrMode = (afAddrMode_t)Addr16Bit;
    281                      zclZigbeeDevice_DstAddr.addr.shortAddr = pRsp->nwkAddr;
    282                      
    283                      // Take the first endpoint, Can be changed to search through endpoints
    284                      zclZigbeeDevice_DstAddr.endPoint = pRsp->epList[0];
    285                      
    286                      HalLedSet( HAL_LED_1, HAL_LED_MODE_ON ); // Light LED
    287                    }
    288                    osal_mem_free( pRsp );
    289                  }
    290                }
    291                break;
    292            }
    293          }
