    225          /*********************************************************************
    226           * @fn      zclZigbeeDevice_ProcessZDOMsgs()
    227           * @brief   Process response messages
    228           */
    229          void zclZigbeeDevice_ProcessZDOMsgs( zdoIncomingMsg_t *inMsg )
    230          {
    231            switch ( inMsg->clusterID )
    232            {
    233              case End_Device_Bind_rsp:
    234                if ( ZDO_ParseBindRsp( inMsg ) == ZSuccess )
    235                {
    236                  HalLedSet( HAL_LED_1, HAL_LED_MODE_ON ); // Light LED
    237                }
    238                #if defined(BLINK_LEDS)
    239                else
    240                {      
    241                  HalLedSet ( HAL_LED_1, HAL_LED_MODE_FLASH ); // Flash LED to show failure
    242                }
    243                #endif
    244                break;
    245          
    246              case Match_Desc_rsp:
    247                {
    248                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    249                  if ( pRsp )
    250                  {
    251                    if ( pRsp->status == ZSuccess && pRsp->cnt )
    252                    {
    253                      zclZigbeeDevice_DstAddr.addrMode = (afAddrMode_t)Addr16Bit;
    254                      zclZigbeeDevice_DstAddr.addr.shortAddr = pRsp->nwkAddr;
    255                      
    256                      // Take the first endpoint, Can be changed to search through endpoints
    257                      zclZigbeeDevice_DstAddr.endPoint = pRsp->epList[0];
    258                      
    259                      HalLedSet( HAL_LED_1, HAL_LED_MODE_ON ); // Light LED
    260                    }
    261                    osal_mem_free( pRsp );
    262                  }
    263                }
    264                break;
    265            }
    266          }
