    253          /*********************************************************************
    254           * @fn      zclZigbeeDevice_ProcessZDOMsgs()
    255           * @brief   Process response messages
    256           */
    257          void zclZigbeeDevice_ProcessZDOMsgs( zdoIncomingMsg_t *inMsg )
    258          {
    259            switch ( inMsg->clusterID )
    260            {
    261              case End_Device_Bind_rsp:
    262                if ( ZDO_ParseBindRsp( inMsg ) == ZSuccess )
    263                {
    264                  HalLedSet( HAL_LED_1, HAL_LED_MODE_ON ); // Light LED
    265                }
    266                #if defined(BLINK_LEDS)
    267                else
    268                {      
    269                  HalLedSet ( HAL_LED_1, HAL_LED_MODE_FLASH ); // Flash LED to show failure
    270                }
    271                #endif
    272                break;
    273          
    274              case Match_Desc_rsp:
    275                {
    276                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    277                  if ( pRsp )
    278                  {
    279                    if ( pRsp->status == ZSuccess && pRsp->cnt )
    280                    {
    281                      zclZigbeeDevice_DstAddr.addrMode = (afAddrMode_t)Addr16Bit;
    282                      zclZigbeeDevice_DstAddr.addr.shortAddr = pRsp->nwkAddr;
    283                      
    284                      // Take the first endpoint, Can be changed to search through endpoints
    285                      zclZigbeeDevice_DstAddr.endPoint = pRsp->epList[0];
    286                      
    287                      HalLedSet( HAL_LED_1, HAL_LED_MODE_ON ); // Light LED
    288                    }
    289                    osal_mem_free( pRsp );
    290                  }
    291                }
    292                break;
    293            }
    294          }
