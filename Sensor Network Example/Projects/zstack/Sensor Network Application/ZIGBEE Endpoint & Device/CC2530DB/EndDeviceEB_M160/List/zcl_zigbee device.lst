    193          void zclZigbeeDevice_ProcessZDOMsgs( zdoIncomingMsg_t *inMsg )
    194          {
    195            switch ( inMsg->clusterID )
    196            {
    197              case End_Device_Bind_rsp:
    198                if ( ZDO_ParseBindRsp( inMsg ) == ZSuccess )
    199                {
    200                  HalLedSet( HAL_LED_1, HAL_LED_MODE_ON ); // Light LED
    201                }
    202                #if defined(BLINK_LEDS)
    203                else
    204                {      
    205                  HalLedSet ( HAL_LED_1, HAL_LED_MODE_FLASH ); // Flash LED to show failure
    206                }
    207                #endif
    208                break;
    209          
    210              case Match_Desc_rsp:
    211                {
    212                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    213                  if ( pRsp )
    214                  {
    215                    if ( pRsp->status == ZSuccess && pRsp->cnt )
    216                    {
    217                      zclZigbeeDevice_DstAddr.addrMode = (afAddrMode_t)Addr16Bit;
    218                      zclZigbeeDevice_DstAddr.addr.shortAddr = pRsp->nwkAddr;
    219                      
    220                      // Take the first endpoint, Can be changed to search through endpoints
    221                      zclZigbeeDevice_DstAddr.endPoint = pRsp->epList[0];
    222                      
    223                      HalLedSet( HAL_LED_1, HAL_LED_MODE_ON ); // Light LED
    224                    }
    225                    osal_mem_free( pRsp );
    226                  }
    227                }
    228                break;
    229            }
    230          }
