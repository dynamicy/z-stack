     77          // Maximums for the data buffer queue
     78          #define NWK_MAX_DATABUFS_WAITING    8     // Waiting to be sent to MAC
     79          #define NWK_MAX_DATABUFS_SCHEDULED  5     // Timed messages to be sent
     80          #define NWK_MAX_DATABUFS_CONFIRMED  5     // Held after MAC confirms
     81          #define NWK_MAX_DATABUFS_TOTAL      12    // Total number of buffers
     82          
     83          // 1-255 (0 -> 256) X RTG_TIMER_INTERVAL
     84          // A known shortcoming is that when a message is enqueued as "hold" for a
     85          // sleeping device, the timer tick may have counted down to 1, so that msg
     86          // will not be held as long as expected. If NWK_INDIRECT_MSG_TIMEOUT is set to 1
     87          // the hold time will vary randomly from 0 - CNT_RTG_TIMER ticks.
     88          // So the hold time will vary within this interval:
     89          // { (NWK_INDIRECT_MSG_TIMEOUT-1)*CNT_RTG_TIMER,
     90          //                                    NWK_INDIRECT_MSG_TIMEOUT*CNT_RTG_TIMER }
     91          #define NWK_INDIRECT_CNT_RTG_TMR    1
     92          // To hold msg for sleeping end devices for 30 secs:
     93          // #define CNT_RTG_TIMER            1
     94          // #define NWK_INDIRECT_MSG_TIMEOUT 30
     95          // To hold msg for sleeping end devices for 30 mins:
     96          // #define CNT_RTG_TIMER            60
     97          // #define NWK_INDIRECT_MSG_TIMEOUT 30
     98          // To hold msg for sleeping end devices for 30 days:
     99          // #define CNT_RTG_TIMER            60
    100          // #define NWK_INDIRECT_MSG_TIMEOUT (30 * 24 * 60)
    101          // Maximum msgs to hold per associated device.
    102          #define NWK_INDIRECT_MSG_MAX_PER    3
    103          // Maximum total msgs to hold for all associated devices.
    104          #define NWK_INDIRECT_MSG_MAX_ALL    \
    105                                      (NWK_MAX_DATABUFS_TOTAL - NWK_INDIRECT_MSG_MAX_PER)
