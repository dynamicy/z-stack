    234          /*********************************************************************
    235           * @fn      zclZigbeeDevice_ProcessZDOMsgs()
    236           * @brief   Process response messages
    237           */
    238          void zclZigbeeDevice_ProcessZDOMsgs( zdoIncomingMsg_t *inMsg )
    239          {
    240            switch ( inMsg->clusterID )
    241            {
    242              case End_Device_Bind_rsp:
    243                if ( ZDO_ParseBindRsp( inMsg ) == ZSuccess )
    244                {
    245                  HalLedSet( HAL_LED_1, HAL_LED_MODE_ON ); // Light LED
    246                }
    247                #if defined(BLINK_LEDS)
    248                else
    249                {      
    250                  HalLedSet ( HAL_LED_1, HAL_LED_MODE_FLASH ); // Flash LED to show failure
    251                }
    252                #endif
    253                break;
    254          
    255              case Match_Desc_rsp:
    256                {
    257                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    258                  if ( pRsp )
    259                  {
    260                    if ( pRsp->status == ZSuccess && pRsp->cnt )
    261                    {
    262                      zclZigbeeDevice_DstAddr.addrMode = (afAddrMode_t)Addr16Bit;
    263                      zclZigbeeDevice_DstAddr.addr.shortAddr = pRsp->nwkAddr;
    264                      
    265                      // Take the first endpoint, Can be changed to search through endpoints
    266                      zclZigbeeDevice_DstAddr.endPoint = pRsp->epList[0];
    267                      
    268                      HalLedSet( HAL_LED_1, HAL_LED_MODE_ON ); // Light LED
    269                    }
    270                    osal_mem_free( pRsp );
    271                  }
    272                }
    273                break;
    274            }
    275          }
