    119          /*
    120           *  Macro for encoding frame control information into internal flags format.
    121           *  Parameter is pointer to the frame.  NOTE!  If either the internal frame
    122           *  format *or* the specification changes, this macro will need to be modified.
    123           */
    124          #define INTERNAL_FCF_FLAGS(p)  ((((p)[1] >> 4) & 0x03) | ((p)[0] & 0x78))
    125          
    126          /*
    127           *  The radio replaces the actual FCS with different information.  This proprietary FCS is
    128           *  the same length as the original and includes:
    129           *    1) the RSSI value
    130           *    2) the average correlation value (used for LQI)
    131           *    3) a CRC passed bit
    132           *
    133           *  These macros decode the proprietary FCS.  The macro parameter is a pointer to the two byte FCS.
    134           */
    135          #define PROPRIETARY_FCS_RSSI(p)                 ((int8)((p)[0]))
    136          #define PROPRIETARY_FCS_CRC_OK(p)               ((p)[1] & PROPRIETARY_FCS_CRC_OK_BIT)
    137          #define PROPRIETARY_FCS_CORRELATION_VALUE(p)    ((p)[1] & ~PROPRIETARY_FCS_CRC_OK_BIT)
