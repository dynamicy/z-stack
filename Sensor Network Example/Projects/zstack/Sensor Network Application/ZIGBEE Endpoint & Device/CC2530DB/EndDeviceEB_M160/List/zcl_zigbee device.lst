    232          static void zclZigbeeDevice_HandleKeys( byte shift, byte keys )
    233          {
    234            zAddrType_t dstAddr;
    235            (void)shift;  // Intentionally unreferenced parameter
    236            
    237            if ( keys & HAL_KEY_SW1 ) // Using timer to control send sensor data event
    238            { 
    239                osal_start_timerEx( zclZigbeeDevice_TaskID, SENSOR_SEND_MSG_EVT, SEND_DISPLAY_TIMER); // Start the OSAL timer
    240                osal_set_event( zclZigbeeDevice_TaskID, SENSOR_SEND_MSG_EVT ); // Action the SENSOR_SEND_MSG_EVT
    241            }
    242            
    243            if ( keys & HAL_KEY_SW2 ) // Atuo binding the device 
    244            {
    245              HalLedSet ( HAL_LED_2, HAL_LED_MODE_BLINK );
    246          
    247              // Initiate a Match Description Request (Service Discovery)
    248              dstAddr.addrMode = AddrBroadcast;
    249              dstAddr.addr.shortAddr = NWK_BROADCAST_SHORTADDR;
    250              ZDP_MatchDescReq( &dstAddr,  NWK_BROADCAST_SHORTADDR, ZCL_HA_PROFILE_ID, ZCLZIGBEEDEV_BINDINGLIST, bindingOutClusters,
    251                                0,  NULL, FALSE );
    252            }
    253          }
